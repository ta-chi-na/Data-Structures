#include <stdio.h>
#include <stdlib.h>

int main(void)

typedef struct node
{
    int number;
    struct node *next;
}
node;

{
    // List of size 0
    node *list = NULL;

    // Number to list
    node *n = malloc(sizeof(node));
    if (n == NULL)
    {
        return 1;
    }
    n->number = 1; // First
    n->next = NULL;

    // Update list to point to new node
    list = n;

    // Add number to list
    n = malloc(sizeof(node));
    if (n == NULL)
    {
        free(list);
        return 1;
    }
    n->number = 2; // Second
    n->next = NULL;
    list->next = n;

     // Add number to list
    n = malloc(sizeof(node));
    if (n == NULL)
    {
        free(list->next);
        free(list);
        return 1;
    }
    n->number = 3;
    n->next = NULL;
    list->next->next = n;

    // Print Numbers using a cryptic 'for loop'
    for (node *tmp = list; tmp != NULL; tmp = tmp->next)
    {
        printf("%i\n", tmp->number)
    }
}
